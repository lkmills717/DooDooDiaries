<div class="navbar navbar-expand-lg navbar-light navbar-lewagon">
  <%= link_to dashboard_path, class: "navbar-brand" do %>
    <%= image_tag "https://i.imgur.com/6Iw3KdB.png" %>
    <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <% if user_signed_in? %>
        <li class="nav-item dropdown">
        <div class="avatar-wrap">
          <%= cl_image_tag current_user.photo.key, height: 100, width: 100, crop: :fill, class: "avatar dropdown-toggle center", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %> 
        </div>
        </li>
        <li class="nav-item">
          <%= link_to "Dashboard", dashboard_path, class: "nav-link, btn-block btn btn-secondary m-1" %>
        </li>
        <li class="nav-item">
          <%= link_to "Profile", profile_path, class: "nav-link, btn-block btn btn-secondary m-1" %>
        </li>
        <li class="nav-item">
          <%= link_to "Log Out", destroy_user_session_path, method: :delete, class: "nav-link, btn-block btn btn-secondary m-1" %>
        </li>
      <% else %>


      <% end %>
    </ul>
  </div>
</div>
